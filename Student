package JavaKamp√ñdev;

public class Student extends User {
	
	private String Course;
	
	public  Student() {
	
	}
		
		public Student(int id, String UserFirstName, String UserLastName, String Email, String password) {
			super(id, UserFirstName, UserLastName, Email, password);
			this.Course = Course;
	}
	
			public String getCourse() {
				return Course;
		}

			public void setCourse(String course) {
				Course = course;
	}

	}
